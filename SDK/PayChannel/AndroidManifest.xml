<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lolofinil.AndroidPG.SDK.PayChannel" >

    <uses-sdk
        android:minSdkVersion="17" />

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:label="AndroidPG.SDK.PayChannel" >
        <activity
            android:name="com.lolofinil.AndroidPG.SDK.PayChannel.activity.MainActivity"
            android:label="AndroidPG.SDK.PayChannel"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.lolofinil.AndroidPG.SDK.PayChannel.activity.WebPageActivity"
            android:label="AndroidPG.SDK.PayChannel"
            >
        </activity>
    </application>

</manifest>
